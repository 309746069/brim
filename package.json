{
  "name": "looky-desktop",
  "description": "Looky Desktop App",
  "repository": "https://github.com/looky/looky-desktop",
  "license": "None",
  "version": "0.1.1",
  "main": "main.js",
  "author": "Looky Labs, Inc.",
  "scripts": {
    "start": "npm-run-all -s build:* -p electron watch:*",
    "test": "jest",
    "electron": "electron .",
    "release": "electron-packager . Looky --out=releases --overwrite --icon=dist/static/looky.smaller.icns",
    "prerelease": "npm run build",
    "build": "npm-run-all -p build:*",
    "watch": "npm-run-all -p watch:*",
    "build:js": "babel src/js -d dist/js",
    "build:css": "node-sass src/css/app.scss dist/css/app.css",
    "build:static": "cpx \"src/static/**\" dist/static",
    "watch:js": "npm run build:js -- --watch --skip-initial-build",
    "watch:css": "npm run build:css -- --watch --source-map-contents sass --source-map true",
    "watch:static": "npm run build:static -- --watch",
    "watch:reload": "livereload dist",
    "lint": "eslint src",
    "format": "prettier 'src/**/*.{js,scss}' --write --loglevel warn"
  },
  "jest": {
    "notify": true,
    "notifyMode": "change",
    "setupTestFrameworkScriptFile": "./src/js/test/setup.js",
    "testURL": "http://localhost",
    "roots": [
      "./src/js"
    ]
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": false
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "electron": "2.0.7",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.2",
    "livereload": "^0.7.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "prettier-eslint": "^8.8.2",
    "text-encoding": "^0.6.4",
    "web-streams-polyfill": "^1.3.2"
  },
  "dependencies": {
    "base64-js": "^1.3.0",
    "boom-js-client": "^1.1.0",
    "classnames": "^2.2.6",
    "d3": "^5.5.0",
    "lodash": "^4.17.10",
    "lookytalk": "git+https://77518fb4e02160c61bd2281151f07007c3a162ad:x-oauth-basic@github.com/looky-cloud/lookytalk.git#a3acfeba6f36bf8076706188f0e50abd8b6362d9",
    "moment": "^2.22.2",
    "prismjs": "^1.15.0",
    "react": "^16.4.2",
    "react-day-picker": "^7.1.10",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  }
}
